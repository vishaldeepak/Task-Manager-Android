<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
<TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="18sp"
    android:text="@string/status"
    android:layout_margin="10dp"/>    

  <TextView
      android:id="@+id/account_statusLogged"
      android:layout_width="111dp"
      android:layout_height="wrap_content"
      android:layout_marginLeft="15dp"
      android:background="#D8D8D8"
      android:textSize="18sp" />
    
    <ViewStub 
        android:id="@+id/AccountviewInflate"
        android:inflatedId="@+id/AccountviewInflated"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      	android:layout="@layout/viewstub_account"  
        />
    
    
    <ViewStub 
        android:id="@+id/AccountviewRegister"
        android:inflatedId="@+id/AccountviewRegisterInflated"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      	android:layout="@layout/viewstub_accountregister"  
        />
    

</LinearLayout>
